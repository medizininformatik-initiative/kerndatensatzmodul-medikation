        "url": "http://hl7.org/fhir/StructureDefinition/translation",
        "extension": [
          {
            "url": "lang",
            "valueCode": "de-DE"
          },
          {
            "url": "content",
            "valueString": "Beschreibt den Sachverhalt, dass ein Patient ein Medikament einnimmt oder auf andere Weise verabreicht bekommt."
