    "resource": [
      {
        "reference": {
          "reference": "StructureDefinition/sd-mii-medikation-medication"
        },
        "name": "SD MII Medikation Medication",
        "description": "Dieses Profil beschreibt die Medikation, die angesetzt, geplant oder verabreicht wird. Es kann sich um ein Fertigarzneimittel oder eine Rezeptur handeln. Auch die Angabe nur des Wirkstoffes ist möglich. Die Angabe mindestens eines Wirkstoffes wird verlangt.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/sd-mii-medikation-medication-administration"
        },
        "name": "SD MII Medikation MedicationAdministration",
        "description": "Dieses Profil beschreibt Medikamentenverabreichungen, einschließlich der Selbstverabreichung von oralen Medikamenten, Injektionen, intravenösen Gaben etc.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/sd-mii-medikation-medication-request"
        },
        "name": "SD MII Medikation MedicationRequest",
        "description": "Dieses Profil beschreibt die Verordnung einer Medikation.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/sd-mii-medikation-medication-statement"
        },
        "name": "SD MII Medikation MedicationStatement",
        "description": "Dieses Profil beschreibt die Verordnung eines Arzneimittels oder den Konsum durch einen Patienten. MedicationStatement kann anzeigen, dass der Patient ein Medikament zur Zeit einnimmt, in der Vergangenheit genommen hat oder in Zukunft einnehmen wird. Dabei kann es sich sowohl um verschreibungspflichtige Medikamente handeln, wie auch um OTC-Präparate, welche ein Patient in Eigenverantwortung einnimmt.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/sd-mii-medikation-medikationsliste"
        },
        "name": "SD MII Medikation Medikationsliste",
        "description": "Liste einzelner Medikationen z.B. zur Dokumentation der Aufnahme- oder Entlassmedikation.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/sd-mii-medikation-wirkstoffrelation"
        },
        "name": "SD MII Medikation Wirkstoffrelation",
        "description": "Die Extension ermöglicht die Zuordnung von einem genauem Wirkstoff (z.B. Salz, Ester) zu einem allgemeinem Wirkstoff.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/sd-mii-medikation-wirkstofftyp"
        },
        "name": "SD MII Medikation Wirkstofftyp",
        "description": "Extension zur Differenzierung des Wirkstofftyps in allgemeinen, genauen oder Kombinationswirkstoff.",
        "exampleBoolean": false
